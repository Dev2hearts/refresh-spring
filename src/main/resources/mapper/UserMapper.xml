<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.refresh.user.UserMapper">
    <insert id="insUser">
        INSERT INTO t_user
        SET nm = #{nm}
            , birth = #{birth}
            , pic = #{pic}
    </insert>
</mapper>